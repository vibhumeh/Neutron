pragma solidity >=0.5.0;

/**
 * @dev Interface of the ERC20 standard as defined in the EIP.
 */
interface BMIERC20 {
   
    //burns tokens
     function burn(address to, uint256 amount) external;
     //mints tokens
     function mint(address to, uint256 amount) external;

}


